# Environment Variables for Development
# Copy this file to .env and update with your actual values
# DO NOT commit .env to version control!

# Database
POSTGRES_DB=evsrs_dev
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres123

# JWT Settings
JWT_ISSUER=EVSRS
JWT_AUDIENCE=EVSRS
JWT_SECRET_KEY=your-jwt-secret-key-here
JWT_ACCESS_TOKEN_EXPIRATION=60
JWT_REFRESH_TOKEN_EXPIRATION=7

# Cloudinary
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret

# Email Settings (Mailgun)
MAILGUN_API_KEY=your-mailgun-api-key
MAILGUN_API_BASE_URI=https://api.mailgun.net
MAILGUN_DOMAIN=your-domain.com
MAILGUN_FROM_EMAIL=noreply@your-domain.com
MAILGUN_FROM_NAME=EVSRS

# Sepay Settings
SEPAY_API_KEY=your-sepay-api-key
SEPAY_API_BASE_URI=https://qr.sepay.vn/img
SEPAY_ACCOUNT_NUMBER=your-account-number
SEPAY_ACCOUNT_NAME=your-account-name
SEPAY_BANK_CODE=your-bank-code

